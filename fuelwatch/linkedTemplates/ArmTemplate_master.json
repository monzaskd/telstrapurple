{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "fuelwatch"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"fuelwatch_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'fuelwatch'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakestored.dfs.core.windows.net"
		},
		"dataLakeStorageAccountKey_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://TelstraPurpleKey.vault.azure.net/"
		},
		"fuelwatch_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.fuelwatch.wa.gov.au/fuelwatch/fuelWatchRSS"
		},
		"fuelwatch_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mainuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fuelwatch_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"fuelwatch_password": {
						"value": "[parameters('fuelwatch_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"dataLakeStorageAccountKey_properties_typeProperties_baseUrl": {
						"value": "[parameters('dataLakeStorageAccountKey_properties_typeProperties_baseUrl')]"
					},
					"fuelwatch_properties_typeProperties_url": {
						"value": "[parameters('fuelwatch_properties_typeProperties_url')]"
					},
					"fuelwatch_properties_typeProperties_userName": {
						"value": "[parameters('fuelwatch_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}